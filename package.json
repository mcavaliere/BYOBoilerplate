{
  "name": "byoboilerplate",
  "version": "0.1.0",
  "description": "A simple, customizable code generator for the command line. Inspired by the rails generate command. Uses the Liquid templating language.",
  "author": "Mike Cavaliere <npm@mikecavaliere.com>",
  "license": "MIT",
  "homepage": "https://github.com/mcavaliere/BYOBoilerplate",
  "repository": "https://github.com/mcavaliere/BYOBoilerplate",
  "bugs": "https://github.com/mcavaliere/BYOBoilerplate/issues",
  "keywords": [
    "boilerplate",
    "cli",
    "code",
    "generator",
    "generation",
    "node",
    "rails",
    "react",
    "react native",
    "redux"
  ],
  "bin": {
    "bgen": "./dist/cli.js"
  },
  "scripts": {
    "bgen": "node ./dist/cli.js",
    "build": "babel src --out-dir dist",
    "build-rollup": "rollup -c",
    "test": "jest --watch --verbose --coverage"
  },
  "jest": {
    "testURL": "http://localhost/",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "chalk": "2.4.1",
    "clui": "0.3.6",
    "fs-extra": "7.0.0",
    "inquirer": "6.2.0",
    "liquid-node": "3.0.0",
    "mkdirp": "0.5.1",
    "node-mkdirp": "0.0.1"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "eslint": "5.6.0",
    "figlet": "1.2.0",
    "jest": "23.6.0",
    "minimist": "1.2.0",
    "mock-fs": "4.7.0",
    "node-glob": "1.2.0",
    "prettier": "1.14.3",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "3.4.0"
  }
}
